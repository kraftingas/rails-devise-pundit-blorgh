<div class="container">
  <div class="row">
    <h3>Listing contacts</h3>
    <div class="column">
      <table class="table table-striped">
        <tbody>
          <tr class="info">
            <td>Name</td>
            <td>Registration Email</td>
            <td>Content</td>
            <td>Action</td>
          </tr>
          <% @contacts.each do |contact| %>
            <tr>
              <%= render contact %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div> 
